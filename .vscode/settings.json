{
  "npm.packageManager": "npm",
  "postcss.validate": false,
  "stylelint.validate": ["css", "scss", "postcss"],
  "editor.codeActionsOnSave": {
    "source.fixAll.stylelint": "explicit"
  },
  "files.associations": {
    "*.css": "postcss"
  },
  "css.validate": false,
  "zencoder.enableRepoIndexing": true
}